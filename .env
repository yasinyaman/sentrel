# Sentrel Local Development Environment

APP_NAME=sentrel
DEBUG=true
LOG_LEVEL=DEBUG
HOST=0.0.0.0
PORT=8000

# Security (development mode - disable auth requirement)
AUTH_REQUIRED=false
ALLOWED_PUBLIC_KEYS=[]
ALLOWED_CORS_ORIGINS=["http://localhost:3000","http://localhost:8080"]
MAX_REQUEST_SIZE=5242880

# Disable Celery for local dev (no Redis required)
USE_CELERY=false

# OpenSearch
OPENSEARCH_HOSTS=["http://localhost:9200"]
OPENSEARCH_VERIFY_CERTS=false

# Projects (empty = allow all)
PROJECT_IDS=[]
